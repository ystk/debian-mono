$(MDOC_SUBCOMMANDS): Makefile
	for script in $(MDOC_SUBCOMMANDS) ; do                  \
		cmd=`echo $$script | sed 's/mdoc-//'` ;         \
		echo "#!/bin/sh" > $$script ;                   \
		echo "exec mdoc $$cmd \"\$$@\"" >> $$script;    \
	done
